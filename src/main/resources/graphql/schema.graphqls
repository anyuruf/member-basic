type Query {
    MemberInfo : membersInfo
    [MemberBasic]: nodes
    [Links]: links
}

type MemberInfo {
    [MemberBasic]: nodes
    [Links]: links
}

type MemberBasic {
    ID : id
    firstName : String
    lastName : String
    dob : String
}

type Link {
    id : ID
    source : String
    target : String
    parent : Parent
}

enum Parent {
    FATHER
    MOTHER
}